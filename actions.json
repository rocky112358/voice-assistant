{
    "manifest": {
        "displayName": "Home automation",
        "invocationName": "Home automation",
        "category": "PRODUCTIVITY"
    },
    "actions": [
        {
            "name": "Wakeup home desktop",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.home.desktop.wakeup",
                "trigger": {
                    "queryPatterns": [
                        "컴퓨터 켜 줘"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "컴퓨터를 켰습니다"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.home.desktop.wakeup"
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "Run calc.exe",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.home.desktop.exec_calc",
                "trigger": {
                    "queryPatterns": [
                        "계산기 켜 줘"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "계산기를 켰습니다"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.home.desktop.exec_calc"
                                }
                            }
                        ]
                    }
                }
            }
        }
    ],
    "locale": "ko"
}

